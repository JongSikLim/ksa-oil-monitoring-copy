    "eject": "react-scripts eject",
    "start:scss": "node-sass -rw src/ --output src/ "
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
